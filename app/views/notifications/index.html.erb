<div class="jumbotron" id="custom_jumbo">
<p id="notice"><%= notice %></p>

<h1>Notifications</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @notifications.each do |notification| %>
    <li class="notification notification--padded">
      <!--initialize the commit _notification in article_observer-->
      <%= sanitize notification.content_notification %>
    </li>
      <tr>
    <% end %>
  </tbody>
</table>

<br>

<%#= link_to 'New Notification', new_notification_path %>
</div>